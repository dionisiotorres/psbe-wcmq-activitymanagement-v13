<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

<t t-name="WCMQ_website_form.eduPartnerSelect">
    <div class="wcmq_edu_partner_select col-md-6 form-field">
        <select name="edu_partner_id" required=""
            class="form-control o_website_form_input"
        >
            <option value="" disabled="" selected="">Institution</option>
            <option value="0">School Not Found</option>
            <t t-foreach="partners" t-as="partner">
                <option t-att-value="partner.id">
                    <t t-esc="partner.display_name"/>
                </option>
            </t>
        </select>
    </div>
</t>

<t t-name="WCMQ_website_form.partnerNameFields">
    <div class="form-group row form-field">
        <div class="col-md-3 text-right">
            <label class="col-form-label" for="partner_firstname">Your Name</label>
        </div>
        <div class="col-md-3 form-field o_website_form_required">
            <input type="text" name="partner_firstname" required=""
                class="form-control o_website_form_input"
                placeholder="First name"
            />
        </div>
        <div class="col-md-3 form-field">
            <input type="text" name="partner_middlename"
                class="form-control o_website_form_input"
                placeholder="Middle name"
            />
        </div>
        <div class="col-md-3 form-field o_website_form_required">
            <input type="text" name="partner_lastname" required=""
                class="form-control o_website_form_input"
                placeholder="Last name"
            />
        </div>
    </div>
</t>

<t t-name="WCMQ_website_form.emailFields">
    <div class="form-group row form-field o_website_form_required" data-check-input-content="equal">
        <div class="col-md-3 text-right">
            <label class="col-form-label" for="partner_email">Your Email</label>
        </div>
        <div class="form-field col-md-4 o_website_form_required">
            <input type="text" name="partner_email" required=""
                class="form-control o_website_form_input"
                placeholder="Email"
            />
        </div>
        <div class="form-field offset-md-1 col-md-4 o_website_form_required">
            <input type="text" name="confirm_partner_email" required=""
                class="form-control o_website_form_input"
                placeholder="Confirm email"
            />
        </div>
    </div>
</t>

<t t-name="WCMQ_website_form.eduPartnerFields">
    <div class="form-group row form-field">
        <div class="col-md-3 text-right">
            <label class="col-form-label" for="edu_partner_county_id">School / University</label>
        </div>
        <div class="col-md-3 form-field o_website_form_required">
            <select name="edu_partner_country_id" required=""
                class="form-control o_website_form_input"
                placeholder="Country"
            >
                <option value="" disabled="" selected="">Country</option>
            </select>
        </div>
    </div>
</t>

<t t-name="WCMQ_website_form.expectedGraduationYearField">
    <div class="form-group row form-field">
        <div class="col-md-3 text-right">
            <label class="col-form-label" for="expected_graduation_year">Expected Year of Graduation</label>
        </div>
        <div class="col-md-3 o_website_form_required">
            <select name="expected_graduation_year" required=""
                class="form-control o_website_form_input"
            >
                <t t-set="cy" t-value="moment().year()"/>
                <t t-foreach="_.range(cy, cy + 7)" t-as="y">
                    <option t-att-value="y"><t t-esc="y"/></option>
                </t>
            </select>
        </div>
    </div>
</t>

<t t-name="WCMQ_website_form.eduPartnerEmpty">
    <div class="wcmq_edu_partner col-md-6 alert alert-warning small">
        No partner found for the selected country. Please fill information here below.
    </div>
</t>

<t t-name="WCMQ_website_form.eduPartnerCreate">
    <div class="wcmq_edu_partner_create from-group row">

        <div class="col-md-9 offset-md-3 form-group form-field o_website_form_required">
            <input type="text" name="edu_partner_name" required=""
                class="form-control o_website_form_input"
                placeholder="Name"
            />
        </div>

        <div class="col-md-9 offset-md-3 form-group form-field">
            <input type="text" name="edu_partner_street"
                class="form-control o_website_form_input"
                placeholder="Address line 1"
            />
        </div>

        <div class="col-md-9 offset-md-3 form-group form-field">
            <input type="text" name="edu_partner_street2"
                class="form-control o_website_form_input"
                placeholder="Address line 2"
            />
        </div>

        <div class="row no-gutters col-md-9 offset-md-3 form-group form-field">
            <div class="col-md form-field o_website_form_required">
                <input type="text" name="edu_partner_city" required=""
                    class="form-control o_website_form_input"
                    placeholder="City"
                />
            </div>
            <div class="col-md form-field">
                <input type="text" name="edu_partner_zip"
                    class="form-control o_website_form_input"
                    placeholder="Zip code"
                />
            </div>
        </div>

        <div class="row no-gutters col-md-9 offset-md-3 form-group form-field">
            <div class="col-md form-field o_website_form_required">
                <select type="text" name="edu_partner_country_id" required="" disabled=""
                    t-att-value="id"
                    class="form-control o_website_form_input"
                >
                    <option t-att-value="id"><t t-esc="name"/></option>
                </select>
            </div>
            <div class="col-md form-field">
                <input type="text" name="edu_partner_state_id"
                    class="form-control o_website_form_input"
                    placeholder="State / Province"
                />
            </div>
        </div>

        <div class="col-md-9 offset-md-3 form-group form-field">
            <input type="text" name="edu_partner_wesite"
                class="form-control o_website_form_input"
                placeholder="Website"
            />
        </div>

        <div class="col-md-9 offset-md-3 form-group form-field">
            <input type="text" name="edu_partner_email"
                class="form-control o_website_form_input"
                placeholder="Email"
            />
        </div>

        <div class="col-md-9 offset-md-3 form-group form-field o_website_form_required">
            <input type="text" name="edu_partner_phone" required=""
                class="form-control o_website_form_input"
                placeholder="Phone"
            />
        </div>

        <div class="row no-gutters col-md-9 offset-md-3 form-group form-field">
            <div class="col-md form-field">
                <select type="text" name="edu_partner_type"
                    t-att-value="id"
                    class="form-control o_website_form_input"
                >
                    <option value="" disabled="" selected="">Type</option>
                    <option value="1">Type 1</option>
                    <option value="2">Type 2</option>
                    <option value="3">Type 3</option>
                    <option value="4">Type 4</option>
                </select>
            </div>
            <div class="col-md form-field">
                <select type="text" name="edu_partner_class_type"
                    t-att-value="id"
                    class="form-control o_website_form_input"
                >
                    <option value="" disabled="" selected="">Class</option>
                    <option value="1">Class 1</option>
                    <option value="2">Class 2</option>
                    <option value="3">Class 3</option>
                    <option value="4">Class 4</option>
                </select>
            </div>
            <div class="col-md form-field">
                <select type="text" name="edu_partner_level"
                    t-att-value="id"
                    class="form-control o_website_form_input"
                >
                    <option value="" disabled="" selected="">Level</option>
                    <option value="1">Level 1</option>
                    <option value="2">Level 2</option>
                    <option value="3">Level 3</option>
                    <option value="4">Level 4</option>
                </select>
            </div>
        </div>

        <div class="row no-gutters col-md-9 offset-md-3 form-group form-field">
            <div class="col-md form-field o_website_form_required">
                <input type="text" id="eduPartnerfromDatePicker" name="edu_partner_from" required=""
                    data-toggle="datetimepicker" data-target="#eduPartnerfromDatePicker"
                    class="form-control datetimepicker-input o_website_form_input o_website_form_date"
                    placeholder="Attended from"
                />
            </div>
            <div class="col-md form-field o_website_form_required">
                <input type="text" id="eduPartnerToDatePicker" name="edu_partner_to" required=""
                    data-toggle="datetimepicker" data-target="#eduPartnerToDatePicker"
                    class="form-control datetimepicker-input o_website_form_input o_website_form_date"
                    placeholder="Attended to"
                />
            </div>
        </div>

    </div>
</t>

</templates>
